{
  "name": "react-weekstrip-daypicker",
  "version": "0.1.7",
  "description": "A day picker in a week based calendar for react. Support both week and working week format.",
  "main": "lib/index.js",
  "scripts": {
    "build-lib": "npm run clean && babel src -d lib --ignore 'src/demo.js'",
    "clean": "rm -rf lib && mkdir lib",
    "demo": "webpack-dev-server --config demo.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Foggyman/react-weekstrip-daypicker.git"
  },
  "keywords": [
    "react",
    "week",
    "calendar",
    "datepicker",
    "picker",
    "component",
    "js",
    "workweek",
    "daypicker",
    "weekpicker",
    "reacjs"
  ],
  "author": "Nicola Crovetti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Foggyman/react-weekstrip-daypicker/issues"
  },
  "homepage": "https://github.com/Foggyman/react-weekstrip-daypicker#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "html-webpack-plugin": "^3.2.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "moment": ">=2.24",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
